{
  "name": "firstuser-sdk-contract",
  "version": "2.0.0",
  "requiredMethods": [
    "init",
    "startPresence",
    "stopPresence",
    "mountHostedChatWidget",
    "startEmbeddedWaitlist",
    "exchangeAccessCode",
    "sendHeartbeat",
    "setPlanTier",
    "getHostedChatWidgetToken"
  ],
  "requiredApiFlows": [
    "/api/firstuser/waitlist/start",
    "/api/firstuser/access/exchange",
    "/api/firstuser/usage/heartbeat",
    "/api/firstuser/users/:externalUserId/plan",
    "/api/firstuser/chat/widget-token"
  ],
  "requiredRealtimeEvents": [
    "presence.heartbeat",
    "live_chat.message",
    "live_chat.typing",
    "live_chat.read"
  ],
  "webhookVerificationHeader": "x-firstuser-signature-sha256"
}
